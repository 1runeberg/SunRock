{
	"name": "Default bindings for Vive Controllers",
	"controller_type": "vive_controller",
	"bindings":
	{
		"/actions/main":
		{
			"sources": [
				{
					"mode": "joystick",
					"path": "/user/hand/left/input/thumbstick",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Teleport (Left)"
						}
					}
				},
				{
					"mode": "joystick",
					"path": "/user/hand/right/input/thumbstick",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Teleport (Right)"
						}
					}
				},
				{
					"mode": "button",
					"path": "/user/hand/left/input/b",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Show UE Hand (Left)"
						}
					}
				},
				{
					"mode": "button",
					"path": "/user/hand/right/input/b",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Show UE Hand (Right)"
						}
					}
				},
				{
					"mode": "button",
					"path": "/user/hand/left/input/a",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Show Steam Hand (Left)"
						}
					}
				},
				{
					"mode": "button",
					"path": "/user/hand/right/input/a",
					"inputs":
					{
						"click":
						{
							"output": "/actions/main/in/Show Steam Hand (Right)"
						}
					}
				},
				{
					"mode": "trackpad",
					"path": "/user/hand/left/input/trackpad",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceLeftXY_axis2d"
						}
					}
				},
				{
					"mode": "joystick",
					"path": "/user/hand/left/input/thumbstick",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceLeftXY_axis2d"
						}
					}
				},
				{
					"mode": "trackpad",
					"path": "/user/hand/left/input/trackpad",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceLeftY_axis"
						}
					}
				},
				{
					"mode": "joystick",
					"path": "/user/hand/left/input/thumbstick",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceLeftY_axis"
						}
					}
				},
				{
					"mode": "trackpad",
					"path": "/user/hand/right/input/trackpad",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceRightXY_axis2d"
						}
					}
				},
				{
					"mode": "joystick",
					"path": "/user/hand/right/input/thumbstick",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceRightXY_axis2d"
						}
					}
				},
				{
					"mode": "trackpad",
					"path": "/user/hand/right/input/trackpad",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceRightY_axis"
						}
					}
				},
				{
					"mode": "joystick",
					"path": "/user/hand/right/input/thumbstick",
					"inputs":
					{
						"position":
						{
							"output": "/actions/main/in/TheForceRightY_axis"
						}
					}
				},
				{
					"mode": "trigger",
					"path": "/user/hand/left/input/trigger",
					"inputs":
					{
						"pull":
						{
							"output": "/actions/main/in/Grab (Left)_axis"
						}
					}
				},
				{
					"mode": "trigger",
					"path": "/user/hand/right/input/trigger",
					"inputs":
					{
						"pull":
						{
							"output": "/actions/main/in/Grab (Right)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/left/input/grip",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/GripForce (Left)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/left/input/grip",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/GripForce (Left)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/right/input/grip",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/GripForce (Right)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/right/input/grip",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/GripForce (Right)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/left/input/trackpad",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/ThumbGrip (Left)_axis"
						}
					}
				},
				{
					"mode": "force_sensor",
					"path": "/user/hand/right/input/trackpad",
					"inputs":
					{
						"force":
						{
							"output": "/actions/main/in/ThumbGrip (Right)_axis"
						}
					}
				}
			],
			"skeleton": [
				{
					"output": "/actions/main/in/skeletonleft",
					"path": "/user/hand/left/input/skeleton/left"
				},
				{
					"output": "/actions/main/in/skeletonright",
					"path": "/user/hand/right/input/skeleton/right"
				}
			],
			"haptics": [
				{
					"output": "/actions/main/out/vibrateleft",
					"path": "/user/hand/left/output/haptic"
				},
				{
					"output": "/actions/main/out/vibrateright",
					"path": "/user/hand/right/output/haptic"
				}
			]
		}
	},
	"description": "SunRock"
}